@page "/Index"

<PageTitle>Index</PageTitle>

<h1>Hello, world!</h1>

<div>
    @foreach (var imageSrc in ImageSources)
    {
        <Image Url="@imageSrc.Url" />
    }
</div>

@code {
    private List<ImageSource> ImageSources = new(){
        new("https://picsum.photos/200/300"),
        new("https://picsum.photos/200/300"),
        new("https://picsum.photos/200/300"),
        new("https://picsum.photos/200/300")
    };

    public record ImageSource(string Url) { }
}